<%= form_with scope: :user, url: sessions_url, local: true do |f| %>
  <fieldset>
    <div>
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>

    <div>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <%= f.submit "Login" %>
  </fieldset>
<% end %>
